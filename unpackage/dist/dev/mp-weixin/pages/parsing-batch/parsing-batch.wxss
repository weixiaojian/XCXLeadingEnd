.parsing {
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
}
.parsing-addr {
  background-color: #ffffff;
  padding: 20rpx;
}
.parsing-addr-nian {
  background-color: #515151;
  color: #FDD605;
  font-size: 30rpx;
  border-radius: 8rpx;
  padding: 20rpx;
  margin-bottom: 20rpx;
}
.parsing-addr-textarea {
  width: 100%;
  background-color: #EAEBEA;
  height: 500rpx;
  border-radius: 8rpx;
  padding: 20rpx;
}
.parsing-button {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #ffffff;
  height: 120rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 18rpx 18rpx 0 0;
  font-weight: bold;
}


.excelMail {
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
}
.excelMail-image {
  width: 100%;
  height: 320rpx;
}
.excelMail-image image {
  width: 100%;
  height: 100%;
}
.excelMail-step {
  display: inline-block;
  text-align: center;
}
.excelMail-step-tit {
  padding: 10rpx 0;
}
.excelMail-step-tit-name {
  color: #FDD605;
  font-size: 30rpx;
  font-weight: 500;
  margin-bottom: 20rpx;
}
.excelMail-step-tit .warningTo {
  color: #ff4041;
  margin: 0 10rpx;
  text-decoration: underline;
}
.excelMail-step-tit image {
  width: 100%;
  height: 120rpx;
  margin-top: 10rpx;
}
.excelMail-button {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #ffffff;
  height: 120rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 18rpx 18rpx 0 0;
}


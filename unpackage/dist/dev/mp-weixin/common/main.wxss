.uni-ellipsis-2 {
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
/* 文本两行溢出隐藏 */
.uni-ellipsis-3 {
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
